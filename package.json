{
  "name": "azure-devops-task-utils",
  "version": "4.0.0",
  "description": "Utility library for Azure DevOps tasks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf ./lib",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidpolaniaac/azure-devops-task-utils.git"
